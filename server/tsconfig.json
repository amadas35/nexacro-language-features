	"compilerOptions": {
		"target": "es2020",
		"lib": ["es2020"],
		"module": "commonjs",
		"moduleResolution": "node",
		"sourceMap": true,
		"strict": true,
		"outDir": "out",
		"rootDir": "src",
		"typeRoots": ["./node_modules/@types"],
